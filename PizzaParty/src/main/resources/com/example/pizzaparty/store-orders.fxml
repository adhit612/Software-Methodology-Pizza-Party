<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.pizzaparty.Controllers.StoreOrdersController">
   <children>
      <Button fx:id="backToMainButton" mnemonicParsing="false" onAction="#backToMainAction" text="Back" />
      <Label layoutX="233.0" layoutY="8.0" text="Store Orders">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="14.0" layoutY="61.0" prefHeight="15.0" prefWidth="134.0" text="Select Order Number: " />
      <ComboBox fx:id="orderNumbersComboBox" layoutX="134.0" layoutY="57.0" prefWidth="150.0" onAction="#orderNumbersComboBoxAction"/>
      <Label layoutX="322.0" layoutY="61.0" text="Order Total: $" />
      <TextField fx:id="orderTotalTextField" layoutX="395.0" layoutY="57.0" />
      <ListView fx:id="orderInformationListView" layoutX="16.0" layoutY="99.0" prefHeight="235.0" prefWidth="568.0" />
      <Button fx:id="cancelOrderButton" layoutX="85.0" layoutY="344.0" mnemonicParsing="false" prefHeight="35.0" prefWidth="98.0" text="Cancel Order" onAction="#cancelOrderButtonAction"/>
      <Button fx:id="exportStoreOrdersButton" layoutX="420.0" layoutY="336.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="134.0" text="Export Store Orders" onAction="#exportStoreOrdersAction"/>
   </children>
</AnchorPane>
